<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.dubbo.provider.user.dao.UserDao">
  <select id="findUserById" parameterType="java.util.Map" resultType="org.dubbo.api.model.UserEntity">
